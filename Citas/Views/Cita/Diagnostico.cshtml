@model IEnumerable<Citas.Models.Cita>;
@{
    ViewData["Title"] = "Index";
}

<h2>Citas Finalizadas</h2>

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Informacion</h3>
            </div>
            <div class="panel-body">
                <table class="table table-hover table-striped" id="mytable">

                    <thead>
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.cliente)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.medico)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Fecha)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Hora)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.diagnostico)
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.cliente)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.medico)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Fecha)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.Hora)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.diagnostico)
                            </td>
                        </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>